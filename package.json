{
  "name": "esurl",
  "version": "0.0.1",
  "description": "URL Shortener for my links",
  "main": "src/server.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "tsnd --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "knex:migrate": "knex migrate:latest --knexfile knexfile.ts",
    "knex:migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts"
  },
  "author": "Emilio",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.3",
    "sqlite3": "^5.0.0"
  }
}
